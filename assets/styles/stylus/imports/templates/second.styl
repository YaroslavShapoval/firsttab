.dial-icon
  float left
  position relative
  display inline-block
  width 160px
  height 120px
  margin 30px 0 0 10px
  background-repeat no-repeat
  -webkit-background-size 100% 100%
  background-size 100% 100%

.dial-description
  display inline-block
  margin 70px 20px 0 0
  height 40px
  font-family Arial

.dial-heading
  color $black
  font-size 1.3em

.dial-address
  color $gray
  font-size 0.9em