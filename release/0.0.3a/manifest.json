{
  "name": "First Deal",
  "version": "0.0.3",
  "description": "New tab page for popularity browsers",

  "incognito": "split",

  "chrome_url_overrides": {
    "newtab": "firstdial.html"
  },

  "background": {
    "scripts": ["js/background.js"],
//    "page": "background.html",
    "persistent": false
  },

  "permissions": [
    "management"
  ],

  "icons": {
    "128": "img/icons/128.png",
    "16": "img/icons/16.png",
    "32": "img/icons/32.png",
    "48": "img/icons/48.png"
  },

  "manifest_version": 2
}